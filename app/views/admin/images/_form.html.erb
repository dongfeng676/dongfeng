<%= bootstrap_form_for([:admin,@image]) do |f| %>
  <%= f.error_messages %>
  <%= f.file_field :name,class:"form-control"%>
  <% if @image.new_record?%>
    <%= f.hidden_field :user_id, value: current_user.id%>
  <% else %>
    <%= f.hidden_field :user_id, value: @image.user_id%>
  <% end %>
  <%= f.submit t('.save'),class:"btn btn-primary"%>
<% end %>