<%= bootstrap_form_for([:admin,@category]) do |f| %>
  <%= f.error_messages %>
  <%= f.text_field :name,class:"form-control"%>
  <%= f.text_field :desc,class:"form-control"%>
  <div class="form-group">
    <div class="col-sm-4 col-sm-offset-3">
      <%= f.submit "保存",class: 'btn btn-primary' %>
    </div>
  </div>
<% end %>